wget http://mirror.koddos.net/gcc/releases/gcc-13.1.0/gcc-13.1.0.tar.xz https://ftp.gnu.org/gnu/binutils/binutils-2.40.tar.xz https://musl.libc.org/releases/musl-1.2.4.tar.gz
tar xf gcc-13.1.0.tar.xz
tar xf binutils-2.40.tar.xz
tar xf musl-1.2.4.tar.gz
cd gcc-13.1.0
sh contrib/download_prerequisites

